
public class Application {
	
	public class RLE{
		
		StringBuffer compressed = new StringBuffer();
		
		public RLE(String phrase)
		{
		  this.phrase = phrase;
		}
		
		public void Compressing() {
			
		for(int i = 0; i < phrase.length; i++) {
			int count = 1;
			if(phrase[i] == phrase[i+1] && (i+1) < phrase.length) {
				count++;
				i++;
			}
			compressed.append(args[i]);
			compressed.append(count);
		}
		 compressed.toString();
		 System.out.printlnt(compressed);
	}
}	
	
	public static void main( String[] args )
	{ 
		if(args == null) {
		System.out.println("error");
		return;
	}
		else if(args.length == 0) {
			String k = "";
			System.out.println(k);	
		}
		else {
			new RLE.Compressing(args);
			
	
	}
 }
}
